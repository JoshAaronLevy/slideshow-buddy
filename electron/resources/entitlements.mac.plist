<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Required for Photos Library access -->
    <key>com.apple.security.assets.photos.read-only</key>
    <true/>
    
    <!-- Required for network access (if needed for image loading) -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <!-- Required for reading user-selected files -->
    <key>com.apple.security.files.user-selected.read-only</key>
    <true/>
    
    <!-- Allow JIT for JavaScript engines -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
    
    <!-- Allow unsigned executable memory for native modules -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Disable library validation for native modules -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
</dict>
</plist>